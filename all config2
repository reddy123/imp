view.xml:
==============

<?xml version="1.0" encoding="UTF-8"?>

<flow xmlns="http://www.springframework.org/schema/webflow"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://www.springframework.org/schema/webflow
                          http://www.springframework.org/schema/webflow/spring-webflow-2.0.xsd">

	<var name="airLineProductVO1" class="com.cnk.travelerpibe.product.air.vo.AirLineProductVO"/>

   <on-start>
		<evaluate expression="displayAirProduct.fetchProductDetails(flowRequestContext)" result="flowScope.airLineProductVO" /> 
			<!--  <input name="someNumber2" value="flowScope.id" type="java.lang.Integer"  />  -->
	 </on-start>
 	
    <view-state id="displayAirProductView" view="displayAirProduct" model="airLineProductVO">
         <transition on="next" to="dispalyAirlineDetailsView" />
         <transition on="edit" to="updateAirProductFlow" >
         	<evaluate expression="updateAirProduct.updateProductDetailsMode(flowScope.airLineProductVO)" result="flowScope.airLineProductVO" />
         </transition>
    </view-state>
  
    <view-state id="dispalyAirlineDetailsView" view="displayAirlineDetails" model="airLineProductVO">
        <transition on="previous" to="displayAirProductView" />
        <!-- <transition on="next" to="displayAirAncillaryView" /> -->	
        <transition on="next" to="displayDeactivateProductView" />
    </view-state>
    
<!--     <view-state id="displayAirAncillaryView" view="displayAirAncillary" model="airLineProductVO">
        <transition on="previous" to="dispalyAirlineDetailsView" />
        <transition on="next" to="displayDeactivateProductView" />
        <transition on="airAncillaryAdded">
			<evaluate expression="displayAirProduct.airAncillary(flowScope.airLineProductVO)" />
