CREATE TABLE TERP_ENTITY 
    ( 
     ENTITY_ID NUMBER  NOT NULL , 
     IS_ENTITY_HIERARCHY NCHAR DEFAULT 'Y' CHECK ( IS_ENTITY_HIERARCHY IN ('N', 'Y')) , 
     ENTITY_META_INFO NVARCHAR2 (2000) , 
     CREATED_BY_ID NUMBER , 
     CREATED_ON_DATE TIMESTAMP , 
     EFFECTIVE_FROM_DATE TIMESTAMP , 
     LAST_UPDATED_BY_ID NUMBER , 
     LAST_UPDATED_ON_DATE TIMESTAMP , 
     IS_DELETED NCHAR DEFAULT 'N' , 
     DELETED_BY_ID NUMBER , 
     DELETED_ON_DATE TIMESTAMP , 
     IS_ACTIVE NCHAR DEFAULT 'Y' , 
     IS_DELETE_LOCKED NCHAR DEFAULT 'N' , 
     IS_UPDATE_LOCKED NCHAR DEFAULT 'N' , 
     IS_TEMPORARY NCHAR DEFAULT 'N' , 
     AUDIT_LOG_ID NUMBER , 
     VERSION_NO NUMBER 
    ) 
;



ALTER TABLE TERP_ENTITY 
    ADD CONSTRAINT CK_TERP_RMH_IS_DELETED 
    CHECK ( IS_DELETED IN ('N', 'Y')) 
;


ALTER TABLE TERP_ENTITY 
    ADD CONSTRAINT CK_TERP_RMH_IS_ACTIVE 
    CHECK ( IS_ACTIVE IN ('N', 'Y')) 
;


ALTER TABLE TERP_ENTITY 
    ADD CONSTRAINT CK_TERP_RMH_IS_DELETE_LOCKED 
    CHECK ( IS_DELETE_LOCKED IN ('N', 'Y')) 
;


ALTER TABLE TERP_ENTITY 
    ADD CONSTRAINT CK_TERP_RMH_IS_UPDATE_LOCKED 
    CHECK ( IS_UPDATE_LOCKED IN ('N', 'Y')) 
;


ALTER TABLE TERP_ENTITY 
    ADD CONSTRAINT CK_TERP_RMH_IS_TEMPORARY 
    CHECK ( IS_TEMPORARY IN ('N', 'Y')) 
;


COMMENT ON TABLE TERP_ENTITY IS 'Travel ERP Reference Master Header'
;


ALTER TABLE TERP_ENTITY 
    ADD CONSTRAINT PK_TERP_ENTITY PRIMARY KEY ( ENTITY_ID ) ;



