     CREATED_BY_ID NUMBER , 
     CREATED_ON_DATE TIMESTAMP , 
     EFFECTIVE_FROM_DATE TIMESTAMP , 
     LAST_UPDATED_BY_ID NUMBER , 
     LAST_UPDATED_ON_DATE TIMESTAMP , 
     IS_DELETED NCHAR DEFAULT 'N' , 
     DELETED_BY_ID NUMBER , 
     DELETED_ON_DATE TIMESTAMP , 
     IS_ACTIVE NCHAR DEFAULT 'Y' , 
     IS_DELETE_LOCKED NCHAR DEFAULT 'N' , 
     IS_UPDATE_LOCKED NCHAR DEFAULT 'N' , 
