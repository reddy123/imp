/**
 * 
 */
package com.cnk.travelerpibe.master.web.util;

/**
 * @author sivaiah.polamreddy
 * 
 * @Description This is general Util class for change dependent masters column
 *              names in insert and update operations (mapping with entity
 *              query).
 * 
 */
public final class ChangeColNames {
	
	private ChangeColNames(){
		
	}

	public static String change(String jsonPros, String tableName) {

		String jsprosvalue = "";

		if (tableName.equalsIgnoreCase("product_category_sub_type")) {

			jsprosvalue = jsonPros
					.replace("PROD_CATGRY_NAME", "PROD_CATGRY_ID");

		} else if (tableName.equalsIgnoreCase("group_company_master")) {

			jsprosvalue = jsonPros.replace("GROUP_OF_COMPANIES_NAME",
					"GROUP_OF_COMPANIES_ID");

		} else if (tableName.equalsIgnoreCase("company_master")) {

			jsprosvalue = jsonPros.replace("GROUP_COMPANY_NAME",
					"GROUP_COMPANY_ID");

		} else if (tableName.equalsIgnoreCase("sbu_master")) {

			jsprosvalue = jsonPros.replace("COMPANY_NAME", "COMPANY_ID");

		} else if (tableName.equalsIgnoreCase("bu_master")) {

			jsprosvalue = jsonPros.replace("SBU_NAME", "SBU_ID");

		} else if (tableName.equalsIgnoreCase("city_master")) {

			jsprosvalue = jsonPros.replace("STATE_NAME", "STATE_ID");

		} else if (tableName.equalsIgnoreCase("state_master")) {

			jsprosvalue = jsonPros.replace("COUNTRY_NAME", "COUNTRY_ID");

		} else if (tableName.equalsIgnoreCase("country_master")) {

			jsprosvalue = jsonPros.replace("CONTINENT_NAME", "CONTINENT_ID");

		}

		else {
			jsprosvalue = jsonPros;
		}

		return jsprosvalue;
	}
}
